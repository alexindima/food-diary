{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "food-diary-web-client": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular/build:application",
                    "options": {
                        "outputPath": {
                            "base": "dist",
                            "browser": ""
                        },
                        "index": "src/index.html",
                        "browser": "src/main.ts",
                        "polyfills": [],
                        "tsConfig": "tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            {
                                "glob": "**/*",
                                "input": "assets",
                                "output": "/assets"
                            }
                        ],
                        "styles": [
                            "src/styles/design-tokens.global.scss",
                            "src/styles/material-theme.scss",
                            "src/styles.scss"
                        ],
                        "allowedCommonJsDependencies": [
                            "cropperjs"
                        ],
                        "scripts": [],
                        "stylePreprocessorOptions": {
                            "includePaths": [
                                "src/styles",
                                "projects/fd-ui-kit/src/lib"
                            ]
                        }
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ],
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2.8MB",
                                    "maximumError": "3.5MB"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "12kB",
                                    "maximumError": "16kB"
                                }
                            ],
                            "outputHashing": "all",
                            "serviceWorker": "ngsw-config.json"
                        },
                        "development": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.ts"
                                }
                            ],
                            "optimization": false,
                            "extractLicenses": false,
                            "sourceMap": true
                        },
                        "staging": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.staging.ts"
                                }
                            ],
                            "outputHashing": "all",
                            "optimization": true,
                            "sourceMap": false
                        }
                    },

                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular/build:dev-server",
                    "options": {
                        "port": 4200
                    },
                    "configurations": {
                        "production": {
                            "buildTarget": "food-diary-web-client:build:production"
                        },
                        "development": {
                            "buildTarget": "food-diary-web-client:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "extract-i18n": {
                    "builder": "@angular/build:extract-i18n"
                },
                "test": {
                    "builder": "@angular/build:karma",
                    "options": {
                        "polyfills": [],
                        "tsConfig": "tsconfig.spec.json",
                        "inlineStyleLanguage": "scss",
                        "stylePreprocessorOptions": {
                            "includePaths": [
                                "src/styles",
                                "projects/fd-ui-kit/src/lib"
                            ]
                        },
                        "assets": [
                            {
                                "glob": "**/*",
                                "input": "assets",
                                "output": "/assets"
                            }
                        ],
                        "styles": [
                            "src/styles/design-tokens.global.scss",
                            "src/styles/material-theme.scss",
                            "src/styles.scss"
                        ],
                        "scripts": []
                    }
                }
            }
        },
        "fd-ui-kit": {
          "projectType": "library",
          "root": "projects/fd-ui-kit",
          "sourceRoot": "projects/fd-ui-kit/src",
          "prefix": "fd",
          "architect": {
            "build": {
              "builder": "@angular/build:ng-packagr",
              "configurations": {
                "production": {
                  "tsConfig": "projects/fd-ui-kit/tsconfig.lib.prod.json"
                },
                "development": {
                  "tsConfig": "projects/fd-ui-kit/tsconfig.lib.json"
                }
              },
              "defaultConfiguration": "production"
            },
            "test": {
              "builder": "@angular/build:karma",
              "options": {
                "tsConfig": "projects/fd-ui-kit/tsconfig.spec.json",
                "polyfills": []
              }
            }
          }
        },
        "fooddiary-admin": {
          "projectType": "application",
          "schematics": {
            "@schematics/angular:component": {
              "style": "scss"
            }
          },
          "root": "projects/fooddiary-admin",
          "sourceRoot": "projects/fooddiary-admin/src",
          "prefix": "app",
          "architect": {
            "build": {
              "builder": "@angular/build:application",
              "options": {
                "outputPath": {
                  "base": "dist-admin",
                  "browser": ""
                },
                "index": "projects/fooddiary-admin/src/index.html",
                "browser": "projects/fooddiary-admin/src/main.ts",
                "polyfills": [],
                "tsConfig": "projects/fooddiary-admin/tsconfig.app.json",
                "inlineStyleLanguage": "scss",
                "assets": [
                  {
                    "glob": "**/*",
                    "input": "projects/fooddiary-admin/public"
                  },
                  {
                    "glob": "**/*",
                    "input": "assets",
                    "output": "/assets"
                  }
                ],
                "styles": [
                  "src/styles/design-tokens.global.scss",
                  "src/styles/material-theme.scss",
                  "projects/fooddiary-admin/src/styles.scss"
                ],
                "stylePreprocessorOptions": {
                  "includePaths": [
                    "src/styles",
                    "projects/fd-ui-kit/src/lib"
                  ]
                }
              },
              "configurations": {
                "production": {
                  "fileReplacements": [
                    {
                      "replace": "projects/fooddiary-admin/src/environments/environment.ts",
                      "with": "projects/fooddiary-admin/src/environments/environment.prod.ts"
                    }
                  ],
                  "budgets": [
                    {
                      "type": "initial",
                      "maximumWarning": "500kB",
                      "maximumError": "1MB"
                    },
                    {
                      "type": "anyComponentStyle",
                      "maximumWarning": "12kB",
                      "maximumError": "16kB"
                    }
                  ],
                  "outputHashing": "all"
                },
                "development": {
                  "fileReplacements": [
                    {
                      "replace": "projects/fooddiary-admin/src/environments/environment.ts",
                      "with": "projects/fooddiary-admin/src/environments/environment.ts"
                    }
                  ],
                  "optimization": false,
                  "extractLicenses": false,
                  "sourceMap": true
                }
              },
              "defaultConfiguration": "production"
            },
            "serve": {
              "builder": "@angular/build:dev-server",
              "options": {
                "port": 4300
              },
              "configurations": {
                "production": {
                  "buildTarget": "fooddiary-admin:build:production"
                },
                "development": {
                  "buildTarget": "fooddiary-admin:build:development"
                }
              },
              "defaultConfiguration": "development"
            },
            "test": {
              "builder": "@angular/build:unit-test"
            }
          }
        }
    },
    "schematics": {
      "@schematics/angular:component": {
        "type": "component"
      },
      "@schematics/angular:directive": {
        "type": "directive"
      },
      "@schematics/angular:service": {
        "type": "service"
      },
      "@schematics/angular:guard": {
        "typeSeparator": "."
      },
      "@schematics/angular:interceptor": {
        "typeSeparator": "."
      },
      "@schematics/angular:module": {
        "typeSeparator": "."
      },
      "@schematics/angular:pipe": {
        "typeSeparator": "."
      },
      "@schematics/angular:resolver": {
        "typeSeparator": "."
      }
    }
}
